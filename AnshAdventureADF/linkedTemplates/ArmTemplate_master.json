{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "AnshAdventureADF"
		},
		"project_db_config_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'project_db_config'"
		},
		"ADLSAnshAdv_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://anshadventure.dfs.core.windows.net/"
		},
		"AzureDatabricks_properties_typeProperties_existingClusterId": {
			"type": "string",
			"defaultValue": "1214-190847-59i9toyj"
		},
		"AzureKeyVault1_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://rupsprojectKV.vault.azure.net/"
		},
		"HTTPAnshAdv_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://raw.githubusercontent.com"
		},
		"project_db_config_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "dbserversai.database.windows.net"
		},
		"project_db_config_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "Project"
		},
		"project_db_config_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "rupsadmin"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "AnshAdventureADF_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"project_db_config_password": {
						"value": "[parameters('project_db_config_password')]"
					},
					"AzureKeyVault1_properties_typeProperties_baseUrl": {
						"value": "[parameters('AzureKeyVault1_properties_typeProperties_baseUrl')]"
					},
					"HTTPAnshAdv_properties_typeProperties_url": {
						"value": "[parameters('HTTPAnshAdv_properties_typeProperties_url')]"
					},
					"project_db_config_properties_typeProperties_server": {
						"value": "[parameters('project_db_config_properties_typeProperties_server')]"
					},
					"project_db_config_properties_typeProperties_database": {
						"value": "[parameters('project_db_config_properties_typeProperties_database')]"
					},
					"project_db_config_properties_typeProperties_userName": {
						"value": "[parameters('project_db_config_properties_typeProperties_userName')]"
					},
					"ADLSAnshAdv_properties_typeProperties_url": {
						"value": "[parameters('ADLSAnshAdv_properties_typeProperties_url')]"
					},
					"AzureDatabricks_properties_typeProperties_existingClusterId": {
						"value": "[parameters('AzureDatabricks_properties_typeProperties_existingClusterId')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}