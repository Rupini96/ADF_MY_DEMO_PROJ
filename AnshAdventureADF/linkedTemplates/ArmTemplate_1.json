{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "AnshAdventureADF"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "[concat(parameters('factoryName'), '/Databricks_Notebook_Transforamtion_Pipeline')]",
			"type": "Microsoft.DataFactory/factories/pipelines",
			"apiVersion": "2018-06-01",
			"properties": {
				"activities": [
					{
						"name": "gwr_group1",
						"type": "ExecutePipeline",
						"dependsOn": [],
						"policy": {
							"secureInput": false
						},
						"userProperties": [],
						"typeProperties": {
							"pipeline": {
								"referenceName": "Databricks_calling_inner_pipeline",
								"type": "PipelineReference"
							},
							"waitOnCompletion": true,
							"parameters": {
								"trigger_identifier": {
									"value": "@pipeline().parameters.trigger_identifier",
									"type": "Expression"
								},
								"group": "group1"
							}
						}
					},
					{
						"name": "gwr_group2",
						"type": "ExecutePipeline",
						"dependsOn": [
							{
								"activity": "gwr_group1",
								"dependencyConditions": [
									"Succeeded"
								]
							}
						],
						"policy": {
							"secureInput": false
						},
						"userProperties": [],
						"typeProperties": {
							"pipeline": {
								"referenceName": "Databricks_calling_inner_pipeline",
								"type": "PipelineReference"
							},
							"waitOnCompletion": true,
							"parameters": {
								"trigger_identifier": {
									"value": "@pipeline().parameters.trigger_identifier",
									"type": "Expression"
								},
								"group": "group2"
							}
						}
					}
				],
				"policy": {
					"elapsedTimeMetric": {}
				},
				"parameters": {
					"trigger_identifier": {
						"type": "string",
						"defaultValue": "gwr"
					}
				},
				"annotations": [],
				"lastPublishTime": "2025-12-15T11:30:24Z"
			},
			"dependsOn": []
		}
	]
}